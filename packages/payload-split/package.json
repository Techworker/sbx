{
  "name": "@pascalcoin-sbx/payload-split",
  "version": "0.0.16",
  "description": "A package that can split pascalcoin payloads.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "PascalCoin",
    "BlockChain",
    "Cryptography"
  ],
  "author": "Benjamin Ansbach <benjaminansbach@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --recursive --require @babel/register --require @babel/polyfill --colors \"./test/**/*.spec.js\"",
    "test:cover": "cross-env NODE_ENV=test nyc --reporter=html mocha --recursive --require @babel/register --require @babel/polyfill --colors \"./test/**/*.spec.js\""
  },
  "dependencies": {
    "@pascalcoin-sbx/common": "^0.0.16",
    "@pascalcoin-sbx/crypto": "^0.0.16"
  }
}
