    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "PascalCoin",
    "BlockChain",
    "Cryptography"
  ],
  "author": "Benjamin Ansbach <benjaminansbach@gmail.com>",
